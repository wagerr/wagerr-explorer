    "i18next-xhr-backend": "^1.5.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.21",
    "mocha": "^5.0.0",
    "moment": "^2.20.1",
    "mongoose": "^5.13.12",
    "morgan": "^1.9.0",
    "node-bitcoin-rpc": "^1.1.3",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "^5.0.0",
    "numeral": "^2.0.6",
    "p-iteration": "^1.1.5",
    "prop-types": "^15.6.0",
    "pubsub-js": "^1.9.1",
    "qrcode": "^1.2.0",
    "query-string": "^7.0.1",
    "react": "^17.0.1",
    "react-countup": "^3.0.3",
    "react-dom": "^17.0.1",
