    "bignumber.js": "^7.2.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.6.0",
    "chai": "^4.3.4",
    "chai-http": "^3.0.0",
    "chart.js": "^2.7.1",
    "classnames": "^2.3.1",
    "compression-webpack-plugin": "^1.1.11",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.4",
    "css-loader": "^3.6.0",
    "endianness": "^8.0.2",
    "ethers": "^5.4.5",
