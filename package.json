    "rxjs": "^7.2.0",
    "sass-loader": "^11.0.1",
    "slick-carousel": "^1.8.1",
    "style-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "wagerrd-rpc": "^0.1.3",
    "web3modal": "git+https://github.com/wagerr/web3modal.git",
    "webpack": "^5.60.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
